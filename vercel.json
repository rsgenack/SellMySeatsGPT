{
  "version": 2,
  "builds": [
    {
      "src": "vercel-adapter.js",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "18.x",
        "includeFiles": ["dist/**"],
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel-adapter.js"
    },
    {
      "src": "/(.*)",
      "dest": "vercel-adapter.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [],
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"]
} 